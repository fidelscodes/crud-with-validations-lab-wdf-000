<h1>All Songs Page</h1>

<h2><%= link_to 'Create New Song', new_song_path %></h2>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Genre</th>
      <th>Released?</th>
      <th>Release Year</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= link_to 'View', song %></td>
    </tr>
    <% end %>
  </tbody>
</table>
